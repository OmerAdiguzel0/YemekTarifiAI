{
  "expo": {
    "name": "AI Yemek Tarifi",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4CAF50"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4CAF50"
      }
    },
    "developmentClient": {
      "silentLaunch": true
    },
    "web": {
      "favicon": null,
      "themeColor": "#4CAF50"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "aiyemektarifi",
    "primaryColor": "#4CAF50",
    "colors": {
      "primary": "#4CAF50"
    }
  }
}
